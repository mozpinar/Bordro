function TOthIncomeOutcomeDef.GetComputationTypeText: string;
var sl : TStringList;
begin
  sl := TStringList.Create;
  try
    Result := '';
    sl.Text := ComponentListDictionary.GetText(ClassName, 'ComputationType.Items.Text');
    if sl.Count<>0 then
      case ComputationType of
      Amount : Result := sl[0];
      Percentage : Result := sl[1];
      ComputeWithScript : Result := sl[2];
      end;
  finally
    sl.Free;
  end;
end;

