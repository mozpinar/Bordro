function TOthIncomeOutcomeDef.GetIncomeOutcomeText: string;
var sl : TStringList;
begin
  Result := '';
  if not(IncomeOutcome.HasValue) then
    Exit;
  sl := TStringList.Create;
  try
    sl.Text := ComponentListDictionary.GetText(ClassName, 'IncomeOutcome.Items.Text');
    if sl.Count<>0 then
      case IncomeOutcome.Value of
      0 : Result := sl[0];
      1 : Result := sl[1];
      end;
  finally
    sl.Free;
  end;
end;

